{
  "name": "go-libp2p",
  "version": "0.0.0",
  "description": "Install the latest go-libp2p binary",
  "main": "src/index.js",
  "scripts": {
    "postinstall": "node src/post-install.js",
    "restore-bin": "git reset -- bin/libp2p && git checkout -- bin/libp2p",
    "test": "tape test/*.js | tap-spec",
    "lint": "tsc --noEmit && standard",
    "prepublishOnly": "tsc"
  },
  "pre-commit": "restore-bin",
  "bin": {
    "libp2p": "bin/p2pd"
  },
  "files": [
    "bin",
    "dist",
    "src",
    "test",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipfs/npm-go-libp2p.git"
  },
  "keywords": [
    "libp2p",
    "install"
  ],
  "author": "Protocol Labs, Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/libp2p/npm-go-libp2p/issues"
  },
  "homepage": "https://github.com/libp2p/npm-go-libp2p",
  "types": "./dist/src/types.d.ts",
  "devDependencies": {
    "@types/got": "^9.6.12",
    "@types/gunzip-maybe": "^1.4.0",
    "@types/tar-fs": "^2.0.1",
    "@types/unzip-stream": "^0.3.1",
    "execa": "^4.0.1",
    "fs-extra": "^9.0.0",
    "pre-commit": "^1.2.2",
    "standard": "^13.1.0",
    "tap-spec": "^5.0.0",
    "tape": "^4.13.2",
    "tape-promise": "^4.0.0",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "cachedir": "^2.3.0",
    "go-platform": "^1.0.0",
    "got": "^11.7.0",
    "gunzip-maybe": "^1.4.2",
    "hasha": "^5.2.2",
    "ipfs-only-hash": "^4.0.0",
    "p-retry": "^4.6.1",
    "pkg-conf": "^3.1.0",
    "tar-fs": "^2.1.0",
    "unzip-stream": "^0.3.0"
  }
}
